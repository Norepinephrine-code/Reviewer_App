# Firebase configuration
FIREBASE_API_KEY="your-firebase-api-key" # API_KEY from Firebase project settings
FIREBASE_AUTH_DOMAIN="your-firebase-auth-domain" # AUTH_DOMAIN used for Firebase auth
FIREBASE_PROJECT_ID="your-firebase-project-id" # PROJECT_ID from Firebase
FIREBASE_STORAGE_BUCKET="your-firebase-storage-bucket" # STORAGE_BUCKET for assets
FIREBASE_MESSAGING_SENDER_ID="your-firebase-sender-id" # MESSAGING_SENDER_ID for push
FIREBASE_APP_ID="your-firebase-app-id" # APP_ID used by Firebase

# AdMob configuration
ADMOB_APP_ID="your-admob-app-id" # AdMob application ID
ADMOB_BANNER_UNIT_ID="your-banner-unit-id" # Banner advertisement unit ID
ADMOB_INTERSTITIAL_UNIT_ID="your-interstitial-unit-id" # Interstitial ad unit ID

# In-app purchase
PLAY_BILLING_KEY="your-google-play-billing-key" # Google Play Billing public key
